{
  "private": true,
  "name": "preact-limeapp",
  "version": "0.2.5",
  "description": "Limeapp webUI and app",
  "license": "GNU",
  "author": "Marcos Gutierrez <gmarcos@altermundi.net>",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --no-prerender",
    "build:production": "preact build --no-prerender --production --no-esm && rm build/*.map",
    "serve": "preact build && serve build --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "translations": "node ./node_modules/i18nline/bin/i18nline.js export",
    "posttranslations": "node ./i18n-diff.js",
    "cz": "git-cz",
    "release": "standard-version",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook",
    "storybook:deploy": "gh-pages -d storybook-static",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/preact": "^5.3.17",
    "@testing-library/preact": "^1.0.2",
    "colors": "^1.4.0",
    "commitizen": "^4.0.0",
    "commitlint": "^8.0.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.0",
    "eslint-config-synacor": "^3.0.2",
    "fetch-mock": "^8.2.0-beta.2",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "per-env": "^1.0.2",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "serve": "^11.1.0",
    "standard-version": "^7.1.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "history": "^4.7.2",
    "i18n-js": "^3.0.3",
    "jssha": "^2.3.1",
    "leaflet.gridlayer.googlemutant": "^0.8.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "preact": "^10.0.1",
    "preact-i18nline": "^2.0.0",
    "preact-render-to-string": "^5.1.0",
    "preact-router": "^3.0.0",
    "preact-router-redux": "^4.1.0",
    "react-redux": "^7.1.3",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.1.0",
    "simple-color-scale": "^1.0.1",
    "simplegrid": "^1.0.2",
    "skeleton-less": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
